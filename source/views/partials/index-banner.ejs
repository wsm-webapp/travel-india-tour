<section class="page-page-banner section-page-banner" data-section-name="page-banner">
  <div class="swiper card-slider card-page-banner" data-section-name="page-banner">
    <div class="swiper-wrapper">
      <% for (var i=1; i <=3; i++ ) { %>
        <div class="swiper-slide page-banner">
          <div class="swiper-card swiper-parallax page-banner">
            <div class="hero d-flex align-items-center">
              <div class="container">
                <div class="row align-items-center justify-content-center">
                  <div class="col-sm-6 font-50 text-shadow">
                    <div class="card-page-banner-content">
                      <h1 class="title itemAnimate text-white" data-aos="fade">
                        
                        <%if (i ===1) { %>Care Connect PRO<% } %>
                        <%if (i ===2) { %>Leading Insight<% } %>
                        <%if (i ===3) { %>Whistle Clean Australia <% } %>
                      </h1>
                      <h2 class="sub-title text-white" data-aos="fade-in" data-aos-easing="ease" data-aos-delay="1200">
                        
                        <%if (i ===1) { %>A mobile app revolutionizing elderly care with personalized management, promoting independence and well-being.<% } %>
                        <%if (i ===2) { %>Redesigning the Leading Insight website to improve navigation, showcase expertise, and build trust through enhanced user experience.<% } %>
                        <%if (i ===3) { %>Revamping Whistle Clean Australia's website to enhance user experience, clarify brand messaging, and highlight eco-friendly initiatives.<% } %>
                      </h2>

                      <div class="card-page-banner-form d-flex" data-aos-anchor-placement="center-bottom" data-aos="fade-in" data-aos-delay="200">
                       <a class="button-frm btn btn-primary btn-lg rounded-5" href="/">View Case Study &nbsp; <i class="fa-solid fa-right-long"></i></a>
                      </div>
                      <!-- <div class="verified d-flex text-white align-items-center"><span class="icons"><img class="svg icon-verified" src="images/verified.svg" /></span> Verified and complaint leads</div> -->
                    
                    </div>
                  </div>
                  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-1 d-none d-sm-block"></div>
                  <div class="col-sm-3">
                    <div class="card-page-banner-images section-bg" data-aos="fade">
                      <img class="svg index-banner-svg" src="
                      <%if (i ===1) { %>images/case-study.png<% } %>
                      <%if (i ===2) { %>images/case-study.png<% } %>
                      <%if (i ===3) { %>images/case-study.png<% } %>
                      " title="" loading="lazy" alt="" />
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <% } %>
    </div>
    <div class="activity-navigation d-flex align-items-center with-pop">
      <div class="button-prev card-slider-prev card-slider-btn">
        <span class="icons-list">
          <div class="title-icons">
            <img class="svg icons-svg" src="images/arrow-icon-right.svg" alt="...">
          </div>
        </span>
      </div>
      <div class="button-next card-slider-next card-slider-btn"><span class="icons-list">
          <div class="title-icons">
            <img class="svg icons-svg" src="images/arrow-icon-right.svg" alt="..."> 
          </div>
        </span>
      </div>
    </div>
  </div>
</section>
<script type='text/javascript'>
  (function ($) {
    const cardPageBanner = new Swiper('.card-page-banner', {
      // Default parameters
      spaceBetween: 0,
      loop: true,
      speed: 1000,
      slidesPerView: 1,
      // autoplay: false,
      navigation: false,
      autoplay: {
        delay: 4000,
        disableOnInteraction: false,
      },
      navigation: {
        nextEl: '.activity-navigation .button-next',
        prevEl: '.activity-navigation .button-prev',
      },
    });
  })(jQuery);
</script>
